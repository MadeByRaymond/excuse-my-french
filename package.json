{
  "name": "ExcuseMyFrench",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "reset": "react-native run-android -- --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "clear-all": "npm cache clean --force && npm install && npm start -- --reset-cache",
    "clean-android": "cd android && gradlew clean && cd ..",
    "clean-assemble-android": "cd android && gradlew clean assembleDebug && cd ..",
    "bundle-release": "cd android && gradlew bundleRelease"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-animatable": "^1.3.3",
    "react-native-audio-recorder-player": "^2.6.2",
    "react-native-device-info": "^8.4.8",
    "react-native-modal": "^13.0.0",
    "react-native-pulse": "^1.0.7",
    "react-native-sound": "^0.11.0",
    "react-native-svg": "^12.1.0",
    "rn-fetch-blob": "^0.12.0",
    "sp-react-native-in-app-updates": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
